  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#12B5E7"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#DAA000"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BE5600"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.control", "meta.tag"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1275e7"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.separator", "meta.delimiter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7C3030"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1275e7"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["entity.name", "support.type", "support.class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00FF00"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1275e7"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": ["source.css support.type.property-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#560061"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9000"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language",
        "comment.block.documentation punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#25E7DE"
      }
    }
  ],
  "editor.semanticHighlighting.enabled": true,
  "semanticHighlighting": true
}
