  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0c2340"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#f9a828"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#f9a828"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#cc241d"
      }
    },
   {
  "name": "Keyword, Storage",
  "scope": ["keyword", "storage.type", "storage.modifier"],
  "settings": {
    "foreground": "#ebdbb2"
  }
},
{
  "name": "Operator, Misc",
  "scope": ["keyword", "constant.other.color", "punctuation", "meta.tag", "punctuation.definition.tag"],
  "settings": {
    "foreground": "#FF"
  }
},
  "tokenColors": [{
    "name": "Comment",
    "scope": ["comment", "punctuation.definition.comment"],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#F9A9288E"
    }
  },{
    "name": "Variables",
    "scope": ["variable", "string constant.other.placeholder"],
    "settings": {
      "foreground": "#0099FF"
    }
  },{
    "name": "Colors",
    "scope": ["constant.other.color"],
    "settings": {
      "foreground": "#ffff"
    }
  },{
    "name": "Invalid",
    "scope": ["invalid", "invalid.illegal"], 
    "settings": {
      "foreground": "#F1730BD8"
    }
  },{
    "name": "Keyword, Storage",
    "scope": ["keyword", "storage.type", "storage.modifier"],
    "settings": {
      "foreground": "#00FF00"
    }
  },{
    "name": "Operator, Misc",
    "scope": ["keyword.control", "constant.other.color", "meta.tag"],
    "settings": {
      "foreground": "#00FF00"
    }
  },{
    "name": "Punctuation",
    "scope": ["punctuation", "meta.separator", "meta.delimiter"],
    "settings": {
      "foreground": "#FF0000"
    }
  },{
    "name": "Function, Special Method",
    "scope": ["entity.name.function", "meta.function-call", "support.function", "keyword.other.special-method"],
    "settings": {
      "foreground": "#00FF00"
    }
  },{
    "name": "Block Level Variables",
    "scope": ["meta.block variable.other"],
    "settings": {
      "foreground": "#1e88e5"
    }
  },{
    "name": "Other Variable, String Link",
    "scope": ["support.other.variable", "string.other.link"],
    "settings": {
      "foreground": "#66FF00"
    }
  },{
    "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
    "scope": ["constant.numeric", "constant.language", "support.constant", "constant.character", "constant.escape",
            "variable.parameter", "keyword.other.unit", "keyword.other"],
    "settings": {
      "foreground": "#FFFF"
    }
  },{
    "name": "String, Symbols, Inherited Class, Markup Heading",
    "scope": ["string", "constant.other.symbol", "constant.other.key", "entity.other.inherited-class", "markup.heading", "markup.inserted.git_gutter", "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"],
    "settings": {
      "foreground": "#FF9D00"
    }
  },{
    "name": "Class, Support",
    "scope": ["entity.name", "support.type", "support.class", "support.orther.namespace.use.php", "meta.use.php", "support.other.namespace.php", "markup.changed.git_gutter", "support.type.sys-types"],
    "settings": {
      "foreground": "#00FF00"
    }
  },{
    "name": "Entity Types",
    "scope": ["support.type"],
    "settings": {
      "foreground": "#00FF9D"
    }
  },{
    "name": "CSS Class and Support",
    "scope": ["source.css support.type.property-name", "source.sass support.type.property-name", "source.scss support.type.property-name", "source.less support.type.property-name", "source.stylus support.type.property-name", "source.postcss support.type.property-name"],
    "settings": {
      "foreground": "#422446"
    }
  },{
    "name": "Sub-methods",
    "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
    "settings": {
      "foreground": "#1e88e5"
    },{
      "name": "Decorators",
      "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
      "settings": {
        "foreground": "#1e88e5"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "foreground": "#f9a234"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#fb4934"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#504945"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "JSON Key - Level 4",
      "scope": ["source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#1e88e5"
       }
  },{
    "name": "Sub-methods",
    "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
    "settings": {
      "foreground": "#1e88e5"
    }
  }],

  "semanticHighlighting": true
}